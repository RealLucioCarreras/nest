DESCRIPTION="Copy tabular data between databases, CSV files and cloud storage"
HOMEPAGE="http://www.dbcrossbar.org"
SRC_URI="$(cargo_crate_uris ${CRATES})"

LICENSE="Apache-2.0"
KEYWORDS=""
SLOT="0"
IUSE=""

DOCS=( {CHANGELOG,README}.md )

src_unpack() {
	git-r3_src_unpack
	cargo_src_unpack
}

src_install() {
	einstalldocs
	pushd dbcrossbar || die "pushd to dbcrossbar failed"
	cargo_src_install
	popd || die "popd from dbcrossbar failed"
}
